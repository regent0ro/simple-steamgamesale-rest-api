{
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "node": true,
    "mocha": true,
    "browser": true
  },
  "plugins": ["prettier"],
  "rules": {
    "no-unused-vars": "off",
    "prettier/prettier": "error",
    "no-console": 0
  },
  "extends": ["eslint:recommended", "problems", "plugin:prettier/recommended"]
}
